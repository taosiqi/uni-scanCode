
.uni-mask {
	position: fixed;
	z-index: 998;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background-color: rgba(0, 0, 0, .3);
}
.uni-popup {
	position: absolute;
	z-index: 999;
	background-color: #ffffff;
	-webkit-box-shadow: 0 0 30rpx rgba(0, 0, 0, .1);
	        box-shadow: 0 0 30rpx rgba(0, 0, 0, .1);
}
